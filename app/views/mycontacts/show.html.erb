	<% else %>
		<%= render partial: 'premium_form' %>
	<% end %>
		<div class="notes">
			<%= render partial: "notes/notes_list", object: @notes%>

		</div>
	<div>
		<p>
			<%= link_to 'New Note', new_user_mycontact_note_path(@user, @mycontact) %> |
