<table>
  <thead>
    <tr>
      <th>Note</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% notes_list.each do |note| %>
    
      <tr>
        <td><%= note.note %></td>
        <td><%= link_to 'Show', user_mycontact_notes_path %></td>
        <td><%= link_to 'Edit', edit_user_mycontact_note_path(@user, @mycontact, note)%></td>
        <td><%= link_to 'Destroy', user_mycontact_note_path(@user, @mycontact, note), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>